; Max Text events

event left
{
    [ key #left ]
    [ key #grayleft ]
}

event fire
{
    [ joyb 1 ]
    [ key #space ]
}

event hello
{
    [ key '2' ]
    [ key '?' ]
;    [ key 'h' key #lalt ]
;    [ key 'h' key #ralt ]
;    [ key #grayenter key #del ]
}

event right
{
    [ key #right ]
    [ key #grayright ]
}

event up
{
    [ key #up ]
    [ key #grayup ]
}

event down
{
    [ key #down ]
    [ key #graydown ]
}

event mmove
{
    [ mouserange 0, 0, 1000, 1000
      mouseb 1 ]
}

event done
{
    [ key #esc ]
    [ mouseb 2 key #lshift ]
}

